config wifi-device  radio0
	option country	BE
	option type     mac80211
	option channel  auto
	option hwmode	11g
	option disabled 0

config wifi-iface
	option device     radio0
	option network    wan
	option mode       sta
	option ssid       zwaluw
	option encryption wep
	option key        4ae56b7820f6b6b3cba78da46b
